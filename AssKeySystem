    Ripple.Parent = parent
    TweenService:Create(Ripple, TweenInfo.new(0.5), {Size = UDim2.new(2, 0, 2, 0), BackgroundTransparency = 1}):Play()
    wait(0.5)
    Ripple:Destroy()
end

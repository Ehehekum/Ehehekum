    if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.V and UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
        KeyInput.Text = game:GetService("ClipboardService"):GetString() or KeyInput.Text
    end
end)


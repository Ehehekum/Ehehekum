-- AG: 3D floating particles
local function Create3DParticle()
    local Particle = Instance.new("Part")
    Particle.Size = Vector3.new(0.2, 0.2, 0.2)
    Particle.Position = Vector3.new(math.random(-10, 10), math.random(-10, 10), math.random(-10, 10))
    Particle.Anchored = true
    Particle.Parent = workspace
    spawn(function()
        while Particle.Parent do
            Particle.Position = Particle.Position + Vector3.new(0, 0.1, 0)
            wait(0.1)
            if Particle.Position.Y > 10 then Particle:Destroy() end
        end
    end)
end
for i = 1, 10 do spawn(Create3DParticle) end


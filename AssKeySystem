-- B: Animated typewriter effect
local function TypewriterEffect(label, text)
    label.Text = ""
    for i = 1, #text do
        label.Text = string.sub(text, 1, i)
        wait(0.05)
    end
end
spawn(function() TypewriterEffect(TitleLabel, TitleLabel.Text) end)


-- Ensure script runs in a Roblox environment
if not game:IsLoaded() then game.Loaded:Wait() end

-- Initialize environment variables for customization
getgenv().KeySystemConfig = getgenv().KeySystemConfig or {
    ValidKeys = {"KEY123", "KEY456", "KEY789"}, -- AE: Multiple keys
    KeyLength = 6, -- Q: Minimum key length
    ExpireDate = "2025-12-31", -- U: Key expiration date (YYYY-MM-DD)
    DevBypass = {"YourUsername"}, -- AF: Developer mode bypass usernames
    WebhookURL = "https://your-discord-webhook.com", -- Z: Discord webhook for logging
    GetKeyURL = "https://your-discord-invite.com", -- S: Link for "Get Key" button
    SoundId = "rbxassetid://123456789", -- D: Custom sound ID for button clicks
    InvalidMessage = "Key salah, coba lagi bozo!", -- E: Custom invalid key message
    Theme = "Dark", -- AN: Theme (Dark, Light, Autumn)
    Language = game:GetService("LocalizationService").SystemLocaleId == "id-ID" and "Indonesian" or "English", -- T: Auto-detect language
    Tips = {"Get your key from our Discord!", "Ensure your key is at least 6 characters.", "Contact support if you face issues."}, -- AQ: Loading tips
}

